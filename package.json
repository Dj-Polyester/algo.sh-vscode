{
  "publisher": "algo-sh",
  "name": "algo-sh",
  "displayName": "algo.sh",
  "description": "",
  "version": "0.1.2",
  "engines": {
    "vscode": "^1.54.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Dj-Polyester/algo.sh-vscode"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:algo-sh.helloWorld",
    "onView:algo-sh-sidebar"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": [
      {
        "title": "algo.sh",
        "properties": {
          "algo-sh.load": {
            "type": "number",
            "default": 2,
            "description": "Specifies how many code blocks to load in a page"
          },
          "algo-sh.highlight": {
            "type": "string",
            "default": "dark",
            "enum": [
              "atelier-sulphurpool-dark",
              "xt256",
              "purebasic",
              "docco",
              "stackoverflow-dark",
              "arta",
              "hopscotch",
              "idea",
              "atelier-savanna-light",
              "sunburst",
              "atelier-forest-light",
              "tomorrow-night-eighties",
              "vs2015",
              "nord",
              "grayscale",
              "atelier-cave-dark",
              "kimbie.dark",
              "routeros",
              "nnfx-dark",
              "school-book",
              "atelier-plateau-dark",
              "ocean",
              "arduino-light",
              "atelier-seaside-dark",
              "ascetic",
              "codepen-embed",
              "magula",
              "xcode",
              "darcula",
              "default",
              "solarized-dark",
              "atelier-seaside-light",
              "atom-one-dark",
              "agate",
              "atelier-heath-light",
              "github",
              "lightfair",
              "srcery",
              "atelier-forest-dark",
              "atelier-plateau-light",
              "atom-one-dark-reasonable",
              "kimbie.light",
              "tomorrow",
              "qtcreator_light",
              "atelier-dune-dark",
              "googlecode",
              "atelier-cave-light",
              "atelier-estuary-light",
              "shades-of-purple",
              "mono-blue",
              "far",
              "gruvbox-light",
              "isbl-editor-dark",
              "school-book",
              "isbl-editor-light",
              "a11y-light",
              "brown-paper",
              "a11y-dark",
              "monokai",
              "androidstudio",
              "github-gist",
              "tomorrow-night",
              "atelier-dune-light",
              "pojoaque",
              "paraiso-light",
              "pojoaque",
              "gruvbox-dark",
              "hybrid",
              "vs",
              "dracula",
              "tomorrow-night-blue",
              "zenburn",
              "rainbow",
              "atelier-lakeside-dark",
              "atelier-savanna-dark",
              "gml",
              "stackoverflow-light",
              "monokai-sublime",
              "brown-papersq",
              "atelier-estuary-dark",
              "atelier-heath-dark",
              "tomorrow-night-bright",
              "railscasts",
              "night-owl",
              "solarized-light",
              "color-brewer",
              "gradient-light",
              "gradient-dark",
              "ir-black",
              "obsidian",
              "an-old-hope",
              "atelier-sulphurpool-light",
              "paraiso-dark",
              "atelier-lakeside-light",
              "qtcreator_dark",
              "dark",
              "nnfx",
              "foundation",
              "lioshi",
              "atom-one-light"
            ],
            "description": "Specifies the highlight theme for rendering code"
          }
        }
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "algo-sh-sidebar-view",
          "title": "algo.sh",
          "icon": "./media/algo.svg"
        }
      ]
    },
    "views": {
      "algo-sh-sidebar-view": [
        {
          "type": "webview",
          "id": "algo-sh-sidebar",
          "name": "algo.sh",
          "icon": "./media/algo.svg",
          "contextualTitle": "algo.sh"
        }
      ]
    },
    "commands": [
      {
        "command": "algo-sh.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.54.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0"
  }
}
